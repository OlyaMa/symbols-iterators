{   
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-plusplus": 0,
        "no-restricted-syntax": 0,
        "func-names": 0,
        "import/extensions": 0
    }    
}